{Nama          : Rivki Anja Afrenda          }
{NPM           : 20.14.1.0049                }
{Nama Program  : Program Luas Persegi Panjang}

program luas_persegi_panjang;
uses crt;

var
  lebar, panjang, luas : integer;

begin
  clrscr;
  writeln ('Program Luas Persegi Panjang');
  writeln ('============================');
  write ('Masukan nilai panjang : ');
  readln (panjang);
  write ('Masukan nilai Lebar : ');
  readln (lebar);
  writeln();
  writeln ('Luas = Panjang x Lebar');
  writeln();
  luas := panjang*lebar;
  writeln ('Luas = ',luas);
  readln
  
end.
